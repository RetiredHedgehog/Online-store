{
  "compilerOptions": {
    "outDir": "./dist/",
    "baseUrl": "src",
    "paths": {
      "interfaces": ["interfaces/*"],
      "classes": ["classes/*"],
      "@/*": ["*"],
    },
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
  }
}
